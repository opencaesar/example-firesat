PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX project: <http://imce.jpl.nasa.gov/foundation/project#>
PREFIX mission: <http://imce.jpl.nasa.gov/foundation/mission#>
PREFIX org: <http://opencaesar.io/organizations/nasa#>
PREFIX pr: <http://purl.org/ontology/prv/core#>
PREFIX pro: <http://purl.org/hpi/patchr#>

Select ?wp ?org ?supplies ?project
WHERE {
  ?p a project:Project.
  ?p rdfs:label ?project.
  ?p project:authorizes ?w.
  ?w a project:WorkPackage .
  ?w rdfs:label ?wp .
  ?w project:isResponsibilityOf ?o .
  ?o rdfs:label ?org .
  ?w project:supplies ?s .
  ?s rdfs:label ?supplies
} ORDER BY ?wp
